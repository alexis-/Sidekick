  <SolidColorBrush x:Key="FilterBuilderGrayBrush10" Color="{DynamicResource Gray10}"/>
  <SolidColorBrush x:Key="FilterBuilderGrayBrush9" Color="{DynamicResource Gray9}"/>
  <SolidColorBrush x:Key="FilterBuilderGrayBrush7" Color="{DynamicResource Gray7}"/>

  <Style x:Key="FilterBuilderComboBoxStyle" BasedOn="{StaticResource {x:Type ComboBox}}" TargetType="{x:Type ComboBox}" />
